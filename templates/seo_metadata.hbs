{{#if includeSeoMetadata}}
<span itemprop="name" content="{{entryMeta.name}}"></span>
<span itemprop="description" content="{{entryMeta.description}}"></span>
<span itemprop="duration" content="{{entryMeta.duration}}"></span>
<span itemprop="thumbnailUrl" content="{{entryMeta.thumbnailUrl}}"></span>
<span itemprop="uploadDate" content="{{entryMeta.uploadDate}}"></span>
<span itemprop="width" content="{{width}}"></span>
<span itemprop="height" content="{{height}}"></span>
{{/if}}